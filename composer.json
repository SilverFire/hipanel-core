        "vova07/yii2-select2-widget": "*",
        "cebe/yii2-gravatar": "*",
        "michelf/php-smartypants": "1.6.*@dev",
        "stichoza/google-translate-php": "~3.0",
        "wbraganca/yii2-dynamicform": "*"
