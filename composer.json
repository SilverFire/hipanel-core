    "require": {
        "php": "^7.1",
        "yiisoft/yii2": "^2.0",
        "yiisoft/yii2-jui": "^2.0",
        "yiisoft/yii2-debug": "^2.0",
        "yiisoft/yii2-imagine": "^2.0",
        "yiisoft/yii2-bootstrap": "^2.0",
        "hiqdev/hisite": "<2.0 || dev-master",
        "hiqdev/hipanel-rbac": "<2.0 || dev-master",
        "hiqdev/hipanel-hiart": "<2.0 || dev-master",
        "hiqdev/yii2-hiam-authclient": "*",
        "hiqdev/yii2-combo": "<2.0 || dev-master",
        "hiqdev/yii2-export": "<2.0 || dev-master",
        "hiqdev/yii2-higrid": "*@dev",
        "hiqdev/yii2-language": "*@dev",
        "hiqdev/yii2-collection": "*@dev",
        "hiqdev/yii2-x-editable": "*@dev",
        "hiqdev/yii2-pnotify": "*",
        "hiqdev/yii2-bootstrap-switch": "<2.0 || dev-master",
        "hiqdev/yii2-asset-hipanel": "<2.0 || dev-master",
        "hiqdev/yii2-asset-lightbox2": "^2.8",
        "hiqdev/yii2-asset-select2-bootstrap3-css": "~1.5 || dev-master",
        "hiqdev/yii2-asset-paulzi-form": "~2.4",
        "hiqdev/yii2-asset-visibilityjs": "*",
        "hiqdev/yii2-reminder": "<2.0 || dev-master",
        "hiqdev/yii2-monitoring": "<2.0 || dev-master",
        "hiqdev/yii2-maintenance": "<2.0 || dev-master",
        "mito/yii2-sentry": "^1.0",
        "kartik-v/yii2-field-range": "^1.3",
        "kartik-v/yii2-widget-fileinput": "^1.0",
        "kartik-v/yii2-widget-switchinput": "^1.3",
        "kartik-v/yii2-widget-datepicker": "^1.3",
        "kartik-v/yii2-widget-timepicker": "^1.0",
        "kartik-v/yii2-widget-datetimepicker": "^1.4",
        "vova07/yii2-select2-widget": "*",
        "cebe/yii2-gravatar": "*",
        "michelf/php-smartypants": "^1.8.0",
        "stichoza/google-translate-php": "~3.0",
        "wbraganca/yii2-dynamicform": "^2.0 || dev-master",
        "bower-asset/css-element-queries": "<1.0",
        "bower-asset/less-space": "*",
        "bower-asset/eonasdan-bootstrap-datetimepicker": "*",
        "bower-asset/components-font-awesome": "*",
        "2amigos/yii2-chartjs-widget": "^2.0",
        "omnilight/yii2-momentjs": "*"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0",
        "hiqdev/hidev-hiqdev": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "hipanel\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "params": "src/config/params.php",
            "common": "src/config/common.php",
            "console": "src/config/console.php",
            "web": "src/config/web.php"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
