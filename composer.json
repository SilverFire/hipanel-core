        "hiqdev/yii2-combo":                "*@dev",
        "hiqdev/yii2-higrid":               "*@dev",
        "hiqdev/yii2-collection":           "*@dev",
        "hiqdev/yii2-menumanager":          "*@dev",
        "hiqdev/yii2-thememanager":         "*@dev",
